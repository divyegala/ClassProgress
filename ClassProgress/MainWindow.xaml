<Window x:Class="ClassProgress.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClassProgress"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Submit" HorizontalAlignment="Left" Margin="220,218,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.152,0.844" Click="button_Click"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="207,174,0,0" VerticalAlignment="Top" Width="105" RenderTransformOrigin="0.506,-1.904"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="18" Margin="207,134,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="105" IsReadOnly="false"/>
        <Label x:Name="label" Content="" HorizontalAlignment="Left" Margin="121,244,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.386,-0.184"/>
    </Grid>
</Window>
